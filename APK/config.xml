<?xml version='1.0' encoding='utf-8'?>
<widget id="com.KR.rythmverse" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RYHTM VERSE</name>
    <description>Juego Ritmico para los chavales</description>
    <author email="dev@cordova.apache.org" href="https://cordova.apache.org">
        Apache Cordova Team
    </author>
    <icon src="www\assets\img\iconAndroid.png" />
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />


<preference name="DisallowOverscroll" value="false" />
<preference name="ScrollEnabled" value="true" />
<preference name="android-usesCleartextTraffic" value="true" />

<preference name="EnableWebViewDebugging" value="true" />

</widget>


